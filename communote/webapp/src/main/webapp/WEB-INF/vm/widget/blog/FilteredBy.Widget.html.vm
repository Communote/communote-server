            <span class="cn-icon"><!-- Empty --></span>
            <span class="cn-icon-label"></span>
        </a>
        <a href="javascript:;" class="cono-filter-settings-startDate-endDate" title="#t('filtered.by.period')" style="display: none">
            <span class="cn-icon"><!-- Empty --></span>
            <span class="cn-icon-label"></span>
