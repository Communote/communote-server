<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.communote.server</groupId>
        <artifactId>communote-server-parent</artifactId>
        <version>3.4.1-SNAPSHOT</version>
    </parent>
    <artifactId>communote-war-parent</artifactId>
    <packaging>pom</packaging>
    <name>Communote - Build - WAR - Parent</name>
    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <phase>initialize</phase>
                        <id>parse-version</id>
                        <goals>
                            <goal>parse-version</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>buildnumber-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <phase>validate</phase>
                        <goals>
                            <goal>create</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <revisionOnScmFailure>local</revisionOnScmFailure>
                    <doCheck>false</doCheck>
                    <doUpdate>false</doUpdate>
                    <shortRevisionLength>0</shortRevisionLength>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <executions>
                    <execution>
                        <id>copy</id>
                        <phase>process-resources</phase>
                        <goals>
                            <goal>copy</goal>
                        </goals>
                        <configuration>
                            <artifactItems>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-plugins-core</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_2
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-plugins-bookmarklet</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-plugins-content-types-extractor</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-plugins-ldap</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-plugin-activity-core</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-plugin-activity-base</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-plugin-atlr</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-plugins-smileys</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-rest-api-1.3-implementation</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-rest-api-2.2-implementation</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-rest-api-2.4-implementation</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-rest-api-3.0-implementation</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_3
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-html-client</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_4
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-embed-plugin</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_4
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-confluence-plugin</artifactId>
                                    <version>${project.version}</version>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_4
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins</groupId>
                                    <artifactId>communote-plugins-webservice-core</artifactId>
                                    <version>${project.version}</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_4
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins.mq</groupId>
                                    <artifactId>communote-plugins-mq-adapter-activemq</artifactId>
                                    <version>${project.version}</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_8
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins.mq</groupId>
                                    <artifactId>communote-plugins-mq-adapter-jms</artifactId>
                                    <version>${project.version}</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_8
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins.mq</groupId>
                                    <artifactId>communote-plugins-mq-message</artifactId>
                                    <version>${project.version}</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_7
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins.mq</groupId>
                                    <artifactId>communote-plugins-mq-message-core</artifactId>
                                    <version>${project.version}</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_7
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins.mq</groupId>
                                    <artifactId>communote-plugins-mq-provider-activemq</artifactId>
                                    <version>${project.version}</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_8
                                    </outputDirectory>
                                </artifactItem>
                                <artifactItem>
                                    <groupId>com.communote.plugins.mq</groupId>
                                    <artifactId>communote-plugins-mq-service</artifactId>
                                    <version>${project.version}</version>
                                    <type>jar</type>
                                    <overWrite>true</overWrite>
                                    <outputDirectory>
                                        ${project.build.directory}/${project.artifactId}-${project.version}/WEB-INF/plugins/level_8
                                    </outputDirectory>
                                </artifactItem>
                            </artifactItems>
                            <overWriteReleases>false</overWriteReleases>
                            <overWriteSnapshots>true
                            </overWriteSnapshots>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>tomcat-maven-plugin</artifactId>
                    <version>1.1</version>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-war-plugin</artifactId>
                    <version>2.3</version>
                </plugin>
                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>build-helper-maven-plugin</artifactId>
                    <version>1.8</version>
                </plugin>
            </plugins>
        </pluginManagement>
    </build>
</project>
